-- Scripts:

local function SFWE_fake_script() -- Proximity.Pimba 
	local script = Instance.new('Script', Proximity)

	for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
		if v.ClassName == "ProximityPrompt" then
			v.HoldDuration = 0    
		end
	end
	
	game:GetService("Workspace").DescendantAdded:Connect(function(Prox)
		if Prox.ClassName == "ProximityPrompt" then
			Prox.HoldDuration = 0    
		end
	end)
end
coroutine.wrap(SFWE_fake_script)()
local function VJHPYOY_fake_script() -- AutoFarm.Farm 
	local script = Instance.new('Script', AutoFarm)

	_G.AutoFarm = true -- true para usar/false para parar
	
	-- Configurações
	local A_1 = game:GetService("Workspace").Delivery["Delivery Quest"].ClickPart.ClickDetector
	local Player = game:GetService("Players").LocalPlayer
	local delay = 1 -- Tempo de Espera (recomendado: 1)
	
	while _G.AutoFarm == true do
		wait(delay)
		Player.Character.HumanoidRootPart.CFrame = CFrame.new(-105.896675, 3.09999919, -135.736084, -0.999657512, -8.68882211e-09, -0.0261689313, -8.3361984e-09, 1, -1.35840184e-08, 0.0261689313, -1.3361217e-08, -0.999657512)
		wait(delay)
		fireclickdetector(A_1)
		wait(delay)
		Player.Character.HumanoidRootPart.CFrame = CFrame.new(155.020126, 3.15187836, -253.247864, -0.0209160857, -6.29727239e-08, 0.999781251, -1.06235596e-07, 1, 6.07639876e-08, -0.999781251, -1.04941414e-07, -0.0209160857)
	end
end
coroutine.wrap(VJHPYOY_fake_script)()
local function PXPBBTF_fake_script() -- Proximity_2.Pimba 
	local script = Instance.new('Script', Proximity_2)

	for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
		if v.ClassName == "ProximityPrompt" then
			v.HoldDuration = 4    
		end
	end
	
	game:GetService("Workspace").DescendantAdded:Connect(function(Prox)
		if Prox.ClassName == "ProximityPrompt" then
			Prox.HoldDuration = 4   
		end
	end)
end
coroutine.wrap(PXPBBTF_fake_script)()
local function RLQZ_fake_script() -- AutoFarm_2.Farm2 
	local script = Instance.new('Script', AutoFarm_2)

	_G.AutoFarm = false -- true para usar/false para parar
	
	-- Configurações
	local A_1 = game:GetService("Workspace").Delivery["Delivery Quest"].ClickPart.ClickDetector
	local Player = game:GetService("Players").LocalPlayer
	local delay = 1 -- Tempo de Espera (recomendado: 1)
	
	while _G.AutoFarm == true do
		wait(delay)
		Player.Character.HumanoidRootPart.CFrame = CFrame.new(-105.896675, 3.09999919, -135.736084, -0.999657512, -8.68882211e-09, -0.0261689313, -8.3361984e-09, 1, -1.35840184e-08, 0.0261689313, -1.3361217e-08, -0.999657512)
		wait(delay)
		fireclickdetector(A_1)
		wait(delay)
		Player.Character.HumanoidRootPart.CFrame = CFrame.new(155.020126, 3.15187836, -253.247864, -0.0209160857, -6.29727239e-08, 0.999781251, -1.06235596e-07, 1, 6.07639876e-08, -0.999781251, -1.04941414e-07, -0.0209160857)
	end
end
coroutine.wrap(RLQZ_fake_script)()
local function VZORWSV_fake_script() -- AC6.Chubalana 
	local script = Instance.new('Script', AC6)

	loadstring(game:HttpGet("https://raw.githubusercontent.com/Luminential/releases/main/ac6_gui.luau", true))()
end
coroutine.wrap(VZORWSV_fake_script)()

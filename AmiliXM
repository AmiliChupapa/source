local function ERHGQ_fake_script() -- FecharMenu.LocalScript 
	local script = Instance.new('LocalScript', FecharMenu)

	local frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
end
coroutine.wrap(ERHGQ_fake_script)()
local function SDEGMT_fake_script() -- Abrir.LocalScript 
	local script = Instance.new('LocalScript', Abrir)

	local Frame = script.Parent.Parent.main
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = true
	end)
end
coroutine.wrap(SDEGMT_fake_script)()
